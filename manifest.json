{
    "manifest_version": 2,
    "name": "Prayer Tab",
    "description": "A new tab override that gives you a",
    "version": "0.0.0.1",
    "permissions": [
        "activeTab", "tabs", "history", "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "browser_action": {
        "default_title": "Prayer Tab"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}